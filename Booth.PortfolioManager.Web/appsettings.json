{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Booth.PortfolioManager.Web.DataImporters.TradingDayImporter": "Information",
      "Booth.PortfolioManager.Web.DataImporters.HistoricalPriceImporter": "Information"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
    "ConnectionString": "mongodb://database:27017",
    "Database": "PortfolioManager",
    "JwtTokenConfiguration": {
      "Issuer": "http://portfolio.boothfamily.id.au",
      "Audience": "http://portfolio.boothfamily.id.au",
      "KeyFile": "config/jwt.key"
    }
  }
}
